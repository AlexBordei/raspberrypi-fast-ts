{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"label": "build raspberrypi-fast-ts",
			"command": "/usr/bin/clang-9",
			"args": [
				"${file}",
				"-O2",
				"-o",
				"${fileDirname}/${fileBasenameNoExtension}"
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build"
		}
	]
}